      ! ---- first derivatives ----
      !   1/6, 1/30, 1/140, 1/630 
      !   ux = D0 ( 1 - (1/6)*dx^2(D+D-) + (1/30)*dx^4 (D+D-)^2 -(1/140)*(D+D-)^3 + ... )
      ux = d100i - (dx(0)**2/6.)*d300i + (dx(0)**4/30.)*d500i - (dx(0)**6/140.)*d700i 
      uy = d010i - (dx(1)**2/6.)*d030i + (dx(1)**4/30.)*d050i - (dx(1)**6/140.)*d070i  

